{
  "version": 2,
  "routes": [
    { "src": "/about", "dest": "/about.html" }
  ],
  "redirects": [
    { "source": "/old-route", "destination": "/new-route", "statusCode": 301 }
  ],
  "headers": [
    { "source": "/api/*", "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }] }
  ]
}
